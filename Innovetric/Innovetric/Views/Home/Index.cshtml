    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-2.1.3.min.js"></script>
    <!-- Modules JavaScript -->
    <script src="~/Modules/innovetricApp.js"></script>
    <script src="~/Modules/innovetricApp.config.js"></script>
    <script src="~/Modules/products-service.js"></script>
    <script src="~/Modules/product-summary.module.js"></script>
    <script src="~/Modules/product-summary.component.js"></script>
    <script src="~/Modules/category-list.module.js"></script>
    <script src="~/Modules/category-list.component.js"></script>
    <script src="~/Modules/product-list.module.js"></script>
    <script src="~/Modules/product-list.component.js"></script>
